public Character[] toCharArr( char[] s ) {

  /* if ( s == null ) {
     return null;
   }*/

   int len = s.length();
   Character[] array = new Character[len];
   for (int i = 0; i < len ; i++) {
      array[i] = new Character(s[i]);
   }

   return array;
}

class Character_str1{
	public static void main(String args[]){
		String s  = "kruthi\n";
		char[] arr = s.toCharArray();
		Character[] charObjectArray = toCharArr(arr);
	}
}
