class staticdemo{
	static int a = 3;
	static int b;
	
	public static void main(String args[]){
		int a = meth();
		System.out.println("23 = " +a);
	}
	
	static int  meth(){
		System.out.println(" a= " + a  + " b = " + b );
		return 23;
	}
	
	static{
	System.out.println("STatic block initailized");
		b= a*3;
	}
	
	static{
	System.out.println("Static block initailized");
		b= a*5;
	}
}
